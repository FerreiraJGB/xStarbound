{
  "xsb": {
    "groups": {
      "camera": { "name": "Camera" },
      "voice": { "name": "Voice" },
      "building": { "name": "Building" }
    },
    "name": "^#f22;xSB-2",
    "binds": {
      "zoomIn": {
        "default": [{
          "type": "key",
          "value": "="
        }],
        "group": "camera",
        "name": "Zoom In"
      },
      "zoomOut": {
        "default": [{
          "type": "key",
          "value": "-"
        }],
        "group": "camera",
        "name": "Zoom Out"
      },
      "incrementalZoomIn": {
        "default": [{
          "type": "key",
          "value": "=",
          "mods" : ["LShift"]
        }],
        "group": "camera",
        "name": "Zoom In Incrementally"
      },
      "incrementalZoomOut": {
        "default": [{
          "type": "key",
          "value": "-",
          "mods" : ["LShift"]
        }],
        "group": "camera",
        "name": "Zoom Out Incrementally"
      },
      "pushToTalk": {
        "default": [],
        "group": "voice",
        "name": "Push To Talk"
      },
      "materialCollisionCycle": {
        "default": [],
        "group": "building",
        "name": "Cycle Material Collision"
      },
      "buildingRadiusGrow": {
        "default": [],
        "group": "building",
        "name": "Increase Building Radius"
      },
      "buildingRadiusShrink": {
        "default": [],
        "group": "building",
        "name": "Decrease Building Radius"
      }
    }
  }
}